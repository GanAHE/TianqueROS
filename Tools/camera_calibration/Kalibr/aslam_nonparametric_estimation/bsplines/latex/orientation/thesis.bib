	Keyword = {Computer Science},
	Note = {10.1007/s11263-011-0465-8},
	Pages = {1-18},
	Publisher = {Springer Netherlands},
	Title = {Efficient Video Rectification and Stabilisation for Cell-Phones},
	
	Year = {2011},
