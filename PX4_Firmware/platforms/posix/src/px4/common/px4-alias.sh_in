# Execute another shell script.
# $1: Path to the script, (optionally starts with /, to match with the NuttX
# scripts)
sh() {
	script="$1"
	case "$script" in
	"/"*)
		script="$script"
		;;
	*)
		script="/$script"
		;;
	esac
	. "$(pwd)$script"
}

