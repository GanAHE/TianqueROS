config NET_IGMP
	bool "IGMPv2 client support"
	default n
	depends on NET_IPv4
	select NET_MCASTGROUP
	select NETDEV_IFINDEX
	---help---
		Enable IGMPv2 client support.

if NET_IGMP

endif # NET_IGMP
