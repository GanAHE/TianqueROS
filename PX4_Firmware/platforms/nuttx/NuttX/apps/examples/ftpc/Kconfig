config EXAMPLES_FTPC
	tristate "FTP client example"
	default n
	---help---
		Enable the FTP client example

if EXAMPLES_FTPC

config EXAMPLES_FTPC_STACKSIZE
	int "FTPC stack size"
	default 4096

endif
